<div class="cal_item">

	<div class="date_box">
		<span class="s_day">{s_day}. {s_mon}</span>
		<span class="s_year">{s_year}</span>
		<span class="enddate">{cal_enddate}</span>
	</div>
	
	<div class="cal_content">
	<span class="showdays">{showdays}</span>
	<a href="{cal_link}"><h3 class="cal_title">{cal_title}</h3></a>
	<div class="cal_text">{cal_text}</div>
		
		
	<span class="cal_cats">{cal_cats}</span>
	</div>
	
	
	<div style="clear:both;"></div>

</div>